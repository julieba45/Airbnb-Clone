Table Users{ //alias 'Owner'
  id integer [PK, increment]
  firstName string [not null]
  lastName string [not null]
  email string
  username string [default: 5]
  password string
}

Ref: Users.id < Spots.owner_id
Ref: Users.id < Reviews.userId
Ref: Spots.id < Bookings.spotId
Ref: Spots.id < Reviews.spotId
Ref: Users.id < Bookings.userId


Table Spots{
  id integer [PK, increment]
  owner_id integer (FK to User)
  address string
  city string
  state string
  country string
  lat decimal
  Ing decimal
  name string
  description string
  price integer
  createdAt timestamp
  updatedAt timestamp

}

Table Reviews {
  id integer
  userId integer (FK to User)
  spotId integer (FK to Spot)
  review string
  stars integer
  createdAt timestamp
  updatedAt timestamp
}

Ref: Spots.id < SpotImages.spotId
Ref: Reviews.id < ReviewImages.reviewId


Table SpotImages {
  id integer
  url string //(alias previewImage)
  preview boolean
  spotId integer
}

Table ReviewImages {
  id integer
  url string
  reviewId integer
}

Table Bookings {
  id integer
  spotId integer (FK to Spot)
  userId integer (FK to User)
  startDate date
  endDate date
}

//stars = avgRating, and avgStarRating
